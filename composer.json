{
    "name": "adminetic/google-analytics",
    "description": "Google Analytics Dashboard For Adminetic Admin Panel",
    "keywords": [
        "adminetic",
        "google-analytics"
    ],
    "homepage": "https://github.com/adminetic/google-analytics",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Pratik Shrestha",
            "email": "pratikdai404@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0",
        "spatie/laravel-analytics": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Adminetic\\GoogleAnalytics\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Adminetic\\GoogleAnalytics\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Adminetic\\GoogleAnalytics\\Provider\\GoogleAnalyticsServiceProvider"
            ]
        }
    }
}